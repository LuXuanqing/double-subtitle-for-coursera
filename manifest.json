{
    "manifest_version": 2,
    "name": "Coursera双字幕",
    "description": "为coursera的视频添加第二字幕",
    "version": "0.3",
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "开启/关闭coursera双字幕",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.coursera.org/learn/*lecture*"
            ],
            "js": [
                "subtitle.js"
            ]
        }
    ]
}